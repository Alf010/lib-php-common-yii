{
    "name": "dreamfactory/lib-php-common-yii",
    "description": "DreamFactory Services Platform(tm) Yii Components Library",
    "config": {
        "discard-changes": true
    },
    "keywords": ["dsp", "sdk", "yii"],
    "license": ["Apache-2.0"],
    "authors": [
        {
            "name": "Jerry Ablan",
            "email": "jerryablan@dreamfactory.com"
        },
        {
            "name": "Todd Appleton",
            "email": "toddappleton@dreamfactory.com"
        },
        {
            "name": "Lee Hicks",
            "email": "leehicks@dreamfactory.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-dist": "true",
    "require": {
        "kisma/kisma": "~0.2",
        "dreamfactory/php-utils": "*",
        "dreamfactory/yii": "~1.1.13.10",
        "dreamfactory/lib-php-common": "~1.5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "@stable",
        "phpunit/phpunit-selenium": "@stable",
        "phpunit/phpunit-story": "@stable",
        "phpunit/dbunit": "@stable"
    },
    "autoload": {
        "psr-4": {
            "DreamFactory\\Yii\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "1.5.x-dev"
        }
    }
}
